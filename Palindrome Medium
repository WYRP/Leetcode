


class Solution {
    public boolean isPalindrome(int x) {
      if(x<0){
          return false;
      } 
      if(x == 0){
          return true;
      }
      int digit = 0;
      int reversed = 0;
      int original = x;
      while(x != 0){
          digit = x % 10;
          reversed = reversed * 10 + digit;
          x = x / 10;
      }
      return reversed == original;
        }
    }
